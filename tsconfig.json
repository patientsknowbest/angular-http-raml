{
  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "allowJs": true,
    "noLib": false,
    "sourceMap": true,
    "outDir": "dist",
    "module": "commonjs",
    "lib": [
      "es2016",
      "dom"],
    "typeRoots": ["./typings/globals", "node_modules/@types", "./custom_typings"]
  },
  "include" : ["*.ts"]
}
